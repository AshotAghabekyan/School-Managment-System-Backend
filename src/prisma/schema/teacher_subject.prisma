



model TeacherOnSubject {
    id Int @id @default(autoincrement()) 
    teacherId Int @map("teacher_id")
    subjectId Int @map("subject_id")

    teacher Teacher @relation(name: "TeacherToTeacherOnSubject", fields: [teacherId], references: [teacherId])
    subject Subject @relation(name: "TeacherToTeacherOnSubject", fields: [subjectId], references: [subjectId])
}